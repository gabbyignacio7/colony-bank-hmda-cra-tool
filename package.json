{
  "name": "colony-bank-hmda-tool",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "HMDA/CRA ETL tool for Colony Bank - processes Encompass and LaserPro exports for CRA Wiz",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabbyignacio7/colony-bank-hmda-cra-tool.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite dev --port 5000",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:client": "vite build",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint client/src --ext .ts,.tsx",
    "lint:fix": "eslint client/src --ext .ts,.tsx --fix",
    "format": "prettier --write 'client/src/**/*.{ts,tsx}'",
    "format:check": "prettier --check 'client/src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm run format:check"
  },
